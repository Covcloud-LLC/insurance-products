{
  "$id": "http://covcloud.com/products/gl/rating/schemas/glratingstep",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "General Liability Rating Step",
  "type": "object",
  "properties": {
    "order": {
      "type": "integer",
      "description": "Rating order",
      "example": 0
    },
    "stepCode": {
      "type": "string",
      "description": "Step code",
      "example": "rateexposure"
    },
    "handler": {
      "type": "object",
      "description": "Handler details",
      "properties": {
        "code": {
          "type": "string",
          "description": "handler code",
          "example": "glexposureroutine"
        },
        "params": {
          "type": "object",
          "description": "Handler params"
        }
      }
    }
  }
}
