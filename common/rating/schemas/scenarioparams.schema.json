{
  "$id": "http://covcloud.com/products/common/rating/schemas/scenarioparams",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Rating scenario params schema",
  "type": "object",
  "properties": {
    "emitCharges": {
      "type": "boolean",
      "description": "Whether or not to emit the charges",
