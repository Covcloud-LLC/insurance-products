{
  "$id": "http://covcloud.com/products/gl/rating/schemas/glexposure",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "General Liability Exposure",
  "type": "object",
  "properties": {
    "amount": {
      "type": "string",
      "format": "cov-decimal",
      "example": "2500000"
    },
    "classCode": {
      "type": "string",
      "description": "Exposure Class Code",
      "example": "50701"
    },
    "effectiveDate": {
      "type": "string",
      "format": "date-time",
      "description": "Exposure effective date",
      "example": "2023-01-01T00:00:00Z"
    },
    "expirationDate": {
      "type": "string",
      "format": "date-time",
      "description": "Exposure expiration date",
      "example": "2024-01-01T00:00:00Z"
    },
    "id": {
      "type": "string",
      "description": "Exposure id unique identifier",
      "example": "exposure-1"
    },
    "modifier": {
      "type": "string",
      "format": "cov-decimal",
      "description": "Exposure level discretionary modifier",
      "example": "-0.1"
    },
    "overrideRate": {
      "type": "string",
      "format": "cov-decimal",
      "description": "If present, this overrides the system calculated rate",
      "example": "2.303"
    },
    "overridePremium": {
      "type": "string",
      "format": "cov-decimal",
      "description": "If present, this overrides exposure level premium",
      "example": "2.303"
    },
    "territoryCode": {
      "type": "string",
      "description": "The relevant territory code",
      "example": "201"
    }
  }
}
